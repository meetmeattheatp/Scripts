local Buttonworkbenchesp = Tab1:CreateButton({
   Name = "Workbench ESP",
   Callback = function()
   local interactables = game.Workspace:FindFirstChild("Interactables")
   if destroyloop == false then
   destroyloop = true
   print("WB ESP: TRUE")
   else
   destroyloop = false
   print("WB ESP: FALSE")
if interactables then
    for _, model in pairs(interactables:GetChildren()) do
        if model:IsA("Model") and model.Name == "Workbench" then
            if model:FindFirstChildOfClass("Highlight") then
               model.Highlight:Destroy()
            end
        end
    end
    end
   end



if interactables then
    for _, model in pairs(interactables:GetChildren()) do
        if model:IsA("Model") and model.Name == "Workbench" then
            if not model:FindFirstChildOfClass("Highlight") then
                local highlight = Instance.new("Highlight")
                highlight.Parent = model
            end
        end
    end
else
    warn("Folder 'interactables' not found!")
end
   end,
})
local Buttonenemyesplol = Tab1:CreateButton({
   Name = "Enemy ESP",
   Callback = function()
local activeHostiles = game.Workspace:FindFirstChild("activeHostiles")

if activeHostiles then
	for _, model in ipairs(activeHostiles:GetChildren()) do
		if model:IsA("Model") then
			local aiType = model:FindFirstChild("ai_type")
			local head = model:FindFirstChild("Head") -- Assumes the model has a part named "Head"

			-- Check if ai_type exists
			if aiType and aiType:IsA("StringValue") and head then
				-- Ensure there's no existing highlight
				if not model:FindFirstChild("Highlight") then
					local highlight = Instance.new("Highlight")
					highlight.Parent = model
					highlight.FillTransparency = 1
				end
			end
		end
	end
	end
      end,
})
