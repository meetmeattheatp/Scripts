local Button8 = Tab4:CreateButton({
   Name = "Infinite Regeneration",
   Callback = function()
   if god == false then
god = true
print("GODMODE: TRUE")
else
god = false
print("GODMODE: FALSE")
end
while wait() do
for i = 1,10 do
workspace.ServerStuff.dealDamage:FireServer("Regeneration")
workspace.ServerStuff.dealDamage:FireServer("Regeneration")
end
if god == false then
break
end
end
      end,
})





